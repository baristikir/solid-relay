{
	"name": "solid-relay",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"example/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"prettier": "latest",
		"turbo": "latest",
		"typescript": "^4.8.4"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"packageManager": "pnpm@7.14.0"
}
